@page
@model Mediflow.Pages.Chemist.OrderInvoiceModel
@{
    ViewData["Title"] = "OrderInvoice";
    Layout = "~/Pages/Shared/Chemist/_ChemistHome.cshtml";
}
@Html.AntiForgeryToken()
<html>
<head>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.3.2/jspdf.min.js"></script>
    <script>
        $(document).ready(function () {
            $('.btnAll').hide();
            $('#invoiceDiv').show();

            $('#track_order').click(function () {

                $('.btnAll').hide();
                $('#hideTrack').show();
            });
            $('#btn_bill').click(function () {

                $('.btnAll').hide();
                $('#invoiceDiv').show();
            });
            $('#btn_return').click(function () {

                $('.btnAll').hide();
                $('#viewInstantReturn').show();
            });
            $('#btn_return_exp').click(function () {

                $('.btnAll').hide();
                $('#viewReturn').show();
            });


            $('.sendOtp').click(function () {

                var orderId = $(this).attr("id");


                $.ajax({


                    type: 'GET',
                    url: 'OrderInvoice?handler=TwoFactor',
                    data: { 'otp': "Send" },

                    cache: false,
                    success: function (data) {

                        alert("OtpSent")

                    },

                });

            });

            //Send Expiry Return Starts
            var arrayExpIds = new Array();
            var arrayExpVals = new Array();
            $('.checkboxSelected').change(function () {
              
                var tempEId,tempEVal;
                if ($(this).prop('checked')) {

                    tempEId = $(this).val();
                    tempEVal = $('#retVal_' + tempEId).val();
                    arrayExpVals.push(tempEVal);
                    arrayExpIds.push(tempEId);

                }
                else {
                    tempId = $(this).val();
                    tempVal = $('#val_' + tempEId).val();
                    arrayExpIds.pop(tempEId);
                    arrayExpVals.pop(tempEVal);
                }
            });

          

            $('#btn_send').click(function () {
               // alert(arrayExpIds.toString() +'-'+ arrayExpVals.toString());
                ////alert(arrayIds);

                $.ajax({

                    type: "GET",
                    traditional: true,
                    url: "OrderInvoice?handler=Return",

                    data: { 'tempIdArray': arrayExpIds, 'tempValArray': arrayExpVals, 'isExpiryReturn': true },

                    cache: false,
                    success: function (data) {
                        alert(data + " Your Expiry Item will be returned");
                        location.reload();
                    }
                });
            });

            //Expiry Return Ends here


            //Instant Return Start
            var arrayIds7Return = new Array();
            var arrayValues7Return = new Array();

            //Instant Return
            $('.ForReturn7Selected').change(function () {

                var tempId;
                if ($(this).prop('checked')) {

                    tempId = $(this).val();
                    tempVal = $('#val_' + tempId).val();
                    arrayValues7Return.push(tempVal);
                    arrayIds7Return.push(tempId);

                }
                else {
                    tempId = $(this).val();
                    tempVal = $('#val_' + tempId).val();
                    arrayIds7Return.pop(tempId);
                    arrayValues7Return.pop(tempVal);
                }

                var tt = arrayIds7Return.toString();
                var vv = arrayValues7Return.toString();
                //alert(tt+''+vv);
            });
            // Instant Return Selected
            $('#ReturnSelected').click(function () {

                if (arrayValues7Return[0] == null && arrayIds7Return[0] == null) {
                    alert("Please Check the box to return")
                }
                else {
                    //alert(arrayIds);
                $.ajax({

                        type: "GET",
                        traditional: true,
                        url: "OrderInvoice?handler=Return",

                        data: { 'tempIdArray': arrayIds7Return, 'tempValArray': arrayValues7Return, 'isExpiryReturn': false },

                        cache: false,
                        success: function (data) {
                            alert(data + " Your Item will be returned");
                            location.reload();
                        }
                    });
                }
            });
            //Instant Return All
            $('#ReturnAll').click(function () {

                var tempArrayIdsAll = new Array();
                var tempArrayValsAll = new Array();
                @{
                    int[] tempAllIds = new int[Model.OrderItemsDetails.Count()];
                   // int[] tempAllValue = new int[];


                        for(int i=0 ; i < Model.OrderItemsDetails.Count();i++)
                        {
                            tempAllIds[i] = @Model.OrderItemsDetails[i].OrderDetail_Id;
                            
                        }
                 }



                tempArrayIdsAll=@Html.Raw(Json.Serialize(tempAllIds));

                for (var i = 0; i < tempArrayIdsAll.length; i++) {
                    var temp = $('#val_' + tempArrayIdsAll[i]).val();
                   
                    tempArrayValsAll.push(temp);
                }

               // alert(tempArrayValsAll.toString());

                $.ajax({

                    type: "GET",
                    traditional: true,
                    url: "OrderInvoice?handler=Return",

                    data: { 'tempIdArray': tempArrayIdsAll, 'tempValArray': tempArrayValsAll, 'isExpiryReturn': false },

                    cache: false,
                    success: function (data) {
                        alert(data + " Your Item will be returned");
                        location.reload();
                    }
                });

            });

            //Instant Return Ends


            //Print Invoice

            $('#printBtn').click(function () {

                debugger;
                var printContents = $('#invoiceDiv').html();
                var originalContents = document.body.innerHTML;

                document.body.innerHTML = printContents;

                window.print();

                document.body.innerHTML = originalContents;

                location.reload();

            });
        });



    </script>
    <style>

        .card {
            z-index: 0;
            background-color: #ECEFF1;
            padding-bottom: 20px;
            border-radius: 10px
        }

        .top {
            padding-top: 40px;
            padding-left: 13% !important;
            padding-right: 13% !important
        }

        #progressbar {
            margin-bottom: 30px;
            overflow: hidden;
            color: #455A64;
            padding-left: 0px;
            margin-top: 30px
        }

            #progressbar li {
                list-style-type: none;
                font-size: 13px;
                width: 20%;
                float: left;
                position: relative;
                font-weight: 400
            }

            #progressbar .step0:before {
                font-family: FontAwesome;
                content: "\f10c";
                color: #fff
            }

            #progressbar li:before {
                width: 40px;
                height: 40px;
                line-height: 45px;
                display: block;
                font-size: 20px;
                background: #99A3A4;
                border-radius: 50%;
                margin: auto;
                padding: 0px
            }

            #progressbar li:after {
                content: '';
                width: 100%;
                height: 12px;
                background: #99A3A4;
                position: absolute;
                left: 0;
                top: 16px;
                z-index: -1
            }

            #progressbar li:last-child:after {
                border-top-right-radius: 10px;
                border-bottom-right-radius: 10px;
                position: absolute;
                left: -50%
            }

            #progressbar li:nth-child(4):after,
            #progressbar li:nth-child(2):after,
            #progressbar li:nth-child(3):after {
                left: -50%
            }

            #progressbar li:first-child:after {
                border-top-left-radius: 10px;
                border-bottom-left-radius: 10px;
                position: absolute;
                left: 50%
            }

            #progressbar li:last-child:after {
                border-top-right-radius: 10px;
                border-bottom-right-radius: 10px
            }

            #progressbar li:first-child:after {
                border-top-left-radius: 10px;
                border-bottom-left-radius: 10px
            }

            #progressbar li.active:before,
            #progressbar li.active:after {
                background: #4fbfb0;
            }

            #progressbar li.active:before {
                font-family: FontAwesome;
                content: "\f00c"
            }

        .icon {
            width: 40px;
            height: 40px;
        }

        .icon-content {
            padding-bottom: 20px
        }

        @@media screen and (max-width: 992px) {
            .icon-content {
                width: 50%
            }
        }
    </style>

</head>

